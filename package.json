{
    "name": "xin_yu-1",
    "version": "1.0.6",
    "author": "xinyu",
    "description": "A collection of Vue 3 components for building modern web applications.",
    "main": "dist/my-components.umd.js",
    "module": "dist/my-components.es.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "docs",
        "!dist/slides"
    ],
    "private": false,
    "type": "module",
    "scripts": {
        "playground": "vite serve playground",
        "prepublishOnly": "npm run build",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "docs": "http-server ./dist -o",
        "create-component": "node scripts/create-component.js",
        "gen-docs": "node scripts/generate-docs.js",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@storybook/vue3": "^8.6.11",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/test-utils": "^2.4.6",
        "@vuedoc/md": "^4.0.0-beta8",
        "@vuedoc/parser": "^4.0.0-beta14",
        "chromatic": "^11.27.0",
        "esbuild": "^0.25.2",
        "eslint": "^9.23.0",
        "eslint-plugin-vue": "^10.0.0",
        "happy-dom": "^17.4.4",
        "http-server": "^14.1.1",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "lint-staged": "^15.5.0",
        "plop": "^4.0.1",
        "prettier": "^3.5.3",
        "size-limit": "^11.2.0",
        "vite": "^6.2.5",
        "vitest": "^3.1.1",
        "vue": "^3.5.13",
        "vue-docgen-api": "^4.79.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,vue}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@vue/compiler-sfc": "^3.5.13",
        "highlight.js": "^11.11.1",
        "monaco-editor-vue3": "^0.1.10",
        "rollup": "^4.39.0",
        "vue-router": "^4.5.0"
    }
}
